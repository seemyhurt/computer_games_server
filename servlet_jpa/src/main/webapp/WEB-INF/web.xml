<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>

    <servlet>
        <servlet-name>ComputerGamesServlet</servlet-name>
        <servlet-class>info.trsis.games.servlets.ComputerGamesServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>ComputerGamesServlet</servlet-name>
        <url-pattern>/games</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>PublishersServlet</servlet-name>
        <servlet-class>info.trsis.games.servlets.PublishersServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>PublishersServlet</servlet-name>
        <url-pattern>/publishers</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DevelopersServlet</servlet-name>
        <servlet-class>info.trsis.games.servlets.DevelopersServlet</servlet-class>
    </servlet>
    
    <servlet-mapping>
        <servlet-name>DevelopersServlet</servlet-name>
        <url-pattern>/developers</url-pattern>
    </servlet-mapping>
</web-app>
